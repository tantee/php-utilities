{
    "name": "tantee/php-utilities",
    "description": "Collection of utilities",
    "keywords": [
        "tantee",
        "php-utilities"
    ],
    "homepage": "https://github.com/tantee/php-utilities",
    "license": "MIT",
    "authors": [
        {
            "name": "TaNteE",
            "email": "poompis@yahoo.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "TaNteE\\PhpUtilities\\": "src"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
